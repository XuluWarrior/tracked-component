{
  "name": "tracked-component",
  "version": "0.0.1",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.mjs"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "watch:cjs": "tsc --watch -p tsconfig.cjs.json",
    "watch:esm": "tsc-watch -p tsconfig.esm.json --onSuccess 'npm run rename:esm'",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json && npm run rename:esm",
    "build": "npm run build:cjs && npm run build:esm",
    "clean": "rimraf dist",
    "rename:esm": "/bin/zsh ./scripts/fix-mjs.sh",
    "prepack": "npm run clean && npm run build"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "description": "",
  "dependencies": {
    "@glimmer/env": "^0.1.7",
    "@glimmer/validator": "^0.92.0",
    "react": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/jest": "^29.5.13",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.1",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-console": "^2.0.0",
    "react-test-renderer": "^18.3.1",
    "rimraf": "^5.0.7",
    "ts-jest": "^29.2.5",
    "tsc-watch": "^6.2.0",
    "typescript": "^5.5.2"
  }
}
